
#include<iostream>
using namespace std;

int
main ()
{
  int f1=1, f2=1, f=0, n;
  cout << "Enter N: ";
  cin >> n;
  while (f<n)  
  {
        f = f1 + f2;
        f1 = f2;
        f2 = f;
    }
   cout << "\nChislo Fibonacci F(K-1): " << f1 << "\nChislo Fibonacci F(K+1): " << f1+f2;
   return 0;
}
