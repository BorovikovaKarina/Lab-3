#include <stdio.h>
#include <iostream>

using namespace std;
 
int main()
{
    int n;
    std:cout << "N:";
    cin >> n;
    int i,r,sum=0,sum1=0;
    for (i=1; i<=n; ++i)
    {
        cout << ">";
        cin>>r;
        sum1+=r;
        if (r==0)
        {
            sum=sum1;
            sum1=0;
        }
    }
    cout << sum;
    return 0;
}
